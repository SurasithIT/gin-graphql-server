type Player {
  id: ID!
  name: String!
  rating: Float!
  team: Team!
}

type Team {
  id: ID!
  name: String!
  country: String!
  players: [Player!]!
}

type Query {
  teams: [Team!]!
  players: [Player!]!
}

input NewTeam {
  name: String!
  country: String!
}

input NewPlayer {
  name: String!
  rating: Float!
  team: Team!
}

type Mutation {
  createTeam(input: NewTeam!): Team
  createPlayer(intpu: NewPlayer!): Player
}
